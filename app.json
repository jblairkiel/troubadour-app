{
  "expo": {
    "name": "troubadour",
    "slug": "my-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/ic_troubadour_logo_no_text-web.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/troubadour_logo_notext_dark-web.png",
      "resizeMode": "contain",
      "backgroundColor": "#808080"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/troubadour_logo_notext_dark-web.png",
        "backgroundColor": "#808080"
      }
    },
    "web": {
      "favicon": "./assets/ic_troubadour_logo_no_text-web.png"
    }
  }
}
